{
	"name": "lcs/simple-router",
	"description": "Simple, fast PHP router that is easy to get integrated and in almost any project. Heavily inspired by the Laravel router.",
	"keywords": [
		"router",
		"router",
		"routing",
		"route",
		"simple-php-router",
		"laravel",
		"pecee",
		"php",
		"framework",
		"url-handling",
		"input-handler",
		"routing-engine",
		"request-handler"
	],
	"license": "MIT",
	"support": {
		"source": "https://github.com/skipperbent/simple-php-router/issues"
	},
	"require": {
		"php": "^8.1",
		"ext-json": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9",
		"mockery/mockery": "^1",
		"phpstan/phpstan": "^1",
		"phpstan/phpstan-phpunit": "^1",
		"phpstan/phpstan-deprecation-rules": "^1",
		"phpstan/phpstan-strict-rules": "^1"
	},
	"scripts": {
		"test": [
			"phpunit tests"
		]
	},
	"autoload": {
		"psr-4": {
			"Pecee\\": "src/Pecee/"
		}
	}
}